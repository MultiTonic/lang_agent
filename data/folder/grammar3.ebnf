
<grammar> ::= <path>

<path> ::= <segment>*

<segment> ::= <direction> <level-type>

<direction> ::= "down" | "up"

<level-type> ::= <physical> | <logical> | <symbolic> | <mental> | <spiritual>

<physical> ::= "physical"

<logical> ::= "logical"

<symbolic> ::= "symbolic"

<mental> ::= "mental"

<spiritual> ::= "spiritual"

